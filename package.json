{
  "name": "clean-node-api",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:watch": "rimraf dist && tsc -w -p tsconfig.build.json",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "dev": "sucrase-node src/main/server.ts",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "lint": "eslint",
    "test": "jest --passWithNoTests --noStackTrace --silent --no-coverage --runInBand",
    "test:verbose": "jest --passWithNoTests --no-coverage --runInBand --detectOpenHandles",
    "test:unit": "npm test -- --watch -c ./jest-unit-config.ts",
    "test:integration": "npm test -- --watch -c ./jest-integration-config.ts",
    "test:ci": "npm test -- --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "12.x"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.6",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.2.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.11",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "git-commit-msg-linter": "^4.5.0",
    "jest": "^29.4.0",
    "mockdate": "^3.0.5",
    "rimraf": "^4.1.2",
    "sucrase": "^3.29.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^4.1.1",
    "nodemon": "^2.0.20",
    "swagger-ui-express": "^4.6.1",
    "validator": "^13.7.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
